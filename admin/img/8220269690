    <script>
        $(function () {
            $("#facebook-register").button();
            initFbConnect();
            function processResponse(response) {
                if (response.status === 'connected') {
                    document.location.href = updateURLParameter("/Profile/FacebookLogin?returnUrl=%2Fgroup%2Fvideos%2F269690", "token", response.authResponse.accessToken);
                }
            }

            $("#facebook-register").click(function (e) {
                e.preventDefault();
                FB.login(processResponse, { scope: 'public_profile,email' });
            });
        });


    </script>






        

            
